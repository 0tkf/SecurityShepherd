<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.app.mobshep.SCDL"
    android:versionCode="1"
    android:versionName="1.0" >



    <uses-sdk
        android:minSdkVersion="8"
        android:targetSdkVersion="22" />

    <permission
        android:name="com.app.mobshep.SCDL.START_ACTIVITY"
        android:icon="@drawable/ic_launcher"
        android:logo="@drawable/ic_launcher"
        android:protectionLevel="dangerous" >
    </permission>
    
          <uses-permission
        android:name="android.permission.WRITE_EXTERNAL_STORAGE"
        android:permission="com.app.mobshep.SCDL.START_ACTIVITY" />

    <application
        android:allowBackup="true"
        android:icon="@drawable/ic_launcher"
        android:label="@string/app_name"
        android:theme="@style/AppTheme" >
        <activity
            android:theme="@style/ShepherdTheme"
            android:name="com.app.mobshep.SCDL.SCData_Leakage"
            android:label="@string/app_name" >
            <intent-filter>
                <action android:name="com.app.mobshep.SCDL.SCData_Leakage" />

                <category android:name="android.intent.category.DEFAULT" />
            </intent-filter>
        </activity>
        <activity
            android:name="com.app.mobshep.SCDL.Splash"
            android:label="@string/app_name" >
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
       
       
    </application>

</manifest>